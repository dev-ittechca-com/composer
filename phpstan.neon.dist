includes:
    - phpstan-baseline.neon
parameters:
    level: 2
    paths:
        - .
    bootstrap: test/bootstrap-phpstan.php
    reportUnmatchedIgnoredErrors: true
    inferPrivatePropertyTypeFromConstructor: true
    excludes_analyse:
        - tmp/*
        - vendor/*
        - node_modules/*
        - examples/openid.php
